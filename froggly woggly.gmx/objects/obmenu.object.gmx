var ypos = ((view_hview)-(armenu[2,0]+(4*spacer))) + spacer/2
var arh = array_height_2d(armenu);
var arl = array_length_2d(armenu,arh);

draw_set_font(font);
{
var i;
 for (i = 0; i &lt; arh+1; i += 1)
 {
    {//draw the buttons and text
    draw_set_alpha(0.3);
    draw_set_colour(make_colour_rgb(45,45,48));
    draw_rectangle(xpos,ypos+(i*spacer),xpos+(armenu[1,i]),ypos+armenu[2,i]+(i*spacer),0);
    draw_set_alpha(1);
    draw_set_colour(c_black);
    draw_text(xpos+((armenu[1,i])/2),ypos+(i*spacer),armenu[0,i]);
    }
if point_in_rectangle(mouse_x,mouse_y,xpos,ypos+(i*spacer),xpos+(armenu[1,i]),ypos+armenu[2,i]+(i*spacer))
    {
    armenu[3,i] = 1 ;
    }
    else
    armenu[3,i] = 0;
    
if armenu[3,i] = 1
    {
    draw_rectangle(xpos,ypos+(i*spacer),xpos+(armenu[1,i]),ypos+armenu[2,i]+(i*spacer),1);
    }
}
}


